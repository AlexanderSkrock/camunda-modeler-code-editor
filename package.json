  "license": "MIT",
  "devDependencies": {
    "@npmcli/arborist": "^9.0.2",
    "@nx/devkit": "^21.0.0",
    "@nx/esbuild": "^21.0.0",
    "@nx/eslint": "20.8.1",
    "archiver": "^7.0.1",
    "esbuild": "^0.20.2",
    "esbuild-plugin-alias": "^0.2.1",
    "esbuild-sass-plugin": "^3.3.1",
    "eslint": "^9.26.0",
    "eslint-plugin-bpmn-io": "^2.2.0",
    "minimist": "^1.2.8",
    "npm-packlist": "^10.0.0",
    "nx": "^21.0.1"
  },
